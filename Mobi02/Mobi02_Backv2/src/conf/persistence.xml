<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="Mobi02PU" transaction-type="JTA">
    <jta-data-source>jdbc/mobi02</jta-data-source>
    <class>Database.Entities.ApiLog</class>
    <class>Database.Entities.PersonalUser</class>
    <class>Database.Entities.R40Statistics</class>
    <class>Database.Entities.Widgets</class>
    <class>Database.Entities.FacebookUserInfo</class>
    <class>Database.Entities.NotificationPreferences</class>
    <class>Database.Entities.TravelTimes</class>
    <class>Database.Entities.UserProperty</class>
    <class>Database.Entities.NmbsStops</class>
    <class>Database.Entities.Property</class>
    <exclude-unlisted-classes>true</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
